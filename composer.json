{
    "name": "hgabka/settings-bundle",
    "type": "symfony-bundle",
    "description": "Settings bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/settings-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2",
        "hgabka/utils-bundle": "^5.0",
        "a2lix/translation-form-bundle": "^3.0",
        "symfony/framework-bundle": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Hgabka\\SettingsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\SettingsBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
